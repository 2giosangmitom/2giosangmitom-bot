{
  "name": "2giosangmitom-bot",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts src/commands/*.ts --outdir dist --target bun --splitting",
    "format": "prettier --write .",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bun": "^1.3.3",
    "@types/node-cron": "^3.0.11",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "fuse.js": "^7.1.0",
    "node-cron": "^4.2.1",
    "zod": "^4.1.13"
  }
}
